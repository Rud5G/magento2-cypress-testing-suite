{
    "name": "elgentos/magento2-cypress-testing-suite",
    "description": "A community-driven Cypress testing suite for Magento 2.",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Peter Jaap Blaakmeer",
            "email": "peterjaap@elgentos.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "extra": {
        "class": "Elgentos\\Cypress\\Composer\\InstallPlugin"
    },
    "autoload": {
        "psr-4": {
            "Elgentos\\Cypress\\": "src"
        }
    }
}
